<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.runtigersrun.runtigers">

    <application
        android:allowBackup="true"
        android:icon="@drawable/appicon"
        android:label="Run Tigers Run"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".Registration" />
        <activity android:name=".Tracks" />
        <activity android:name=".TrackEditor" />
        <activity android:name=".UserInfo" />
        <activity android:name=".EditUser" />
        <activity android:name=".Groups" />
        <activity android:name=".GroupEditor"></activity>
    </application>

</manifest>